{
    "manifest_version": 2,
    "name": "mailTrack",
    "version": "1.0",
    "default_locale": "en",
    "description": "Let's Track mail sent from Gmail!",
    "icons": "",
    "browser_action":{
        "default_popup":"index.html"
    },
    "permissions": [
        "activeTab", "http://*/*", "https://*/*"
    ]
}